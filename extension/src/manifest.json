{
    "name": "Privacy Bird",
    "version": "1.0",
    "description": "Privacy Bird Extension",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "http://localhost*"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "background.js"
            ],
            "run_at": "document_end"
        }
    ],
    "background": {
        "scripts": [
            "third-party/jquery-3.3.1.min.js"
        ]
    }
}